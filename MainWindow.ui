*{
	font: 14px &quot;Microsoft YaHei&quot;;
	color: rgb(30,40,70);
	background-color: rgb(240, 240, 245);
	outline: none;
	border: none;
}

/* Label */
QLabel
{
	background:transparent;
}

/* ToolTip */
QToolTip {
	color: rgb(30,40,70);
    border: 1px solid rgba(180, 180, 180, 100);
    padding: 5px;
    border-radius: 4px;
    opacity: 150;
}

/* List View */
QListView::item
{
	background-color: rgb(255, 255, 255);
	padding: 10px;
	border-radius: 4px;
	border: 0 solid transparent;
}
QListView::item:hover {
	padding: 8px;
    border: 2px solid rgb(85,118,189);
}
QListView::item:selected {
	padding: 8px;
    border: 2px solid rgb(187,200,228);
}
QListView::item:selected:!active {
	padding: 8px;
    border: 2px solid #fff;
}
QListView::item:selected:active {
	padding: 8px;
    border: 2px solid rgb(187,200,228);
}
QListView::item:hover {
	padding: 8px;
    border: 2px solid rgb(224,227,241);
}

/* Menu */
QMenu{
	background-color: #fff;
	padding-top: 8px;
	padding-bottom: 8px;
}
QMenu::item{
	padding: 5px 10px 5px 25px;
	border-left: 10px solid transparent;
}
QMenu::item:selected{
	color: #fff;
	background-color: #5576be;
}
QMenu::item:pressed{
	background-color: #445e97;
}
QMenu::separator{
    height: 1px;
    background: #eee;
    margin-left: 10px;
    margin-right: 10px;
}
QMenu:disabled{
	color:  rgba(50,50,80,80);
}

/* LineEdit */
QLineEdit
{
	height: 46px;
	padding: 0px 20px;
	border-radius: 23px;
	background: white;
}
/*QLineEdit::hover
{
	border: 1px solid rgb(85,118,189);;
}*/

/* ScrollBar  */
QScrollBar:vertical
{
    background: white;
    width: 10px;
}
QScrollBar::handle:vertical
{
    background: rgb(224,227,241);
    min-height: 10px;
}
QScrollBar::handle:hover
{
	background: rgb(187,200,228);
}
QScrollBar::add-line:vertical
{
    height: 0;
}
QScrollBar::sub-line:vertical
{
    height: 0;
}

QScrollBar:horizontal
{
    background: white;
    height: 10px;
}
QScrollBar::handle:horizontal
{
    background: rgb(224,227,241);
    min-width: 10px;
}
QScrollBar::horizontal:hover
{
	background: rgb(187,200,228);
}
QScrollBar::add-line:horizontal
{
    width: 0;
}
QScrollBar::sub-line:horizontal
{
    width: 0;
}

